{
  "name": "p5js-circles",
  "version": "0.1.0",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "start": "parcel ./src/index.html",
    "build": "npm run prettier-lint-fix && npm run clean && parcel build ./src/*.html && cp ./src/p5js-circles.png ./dist/p5js-circles.png",
    "lint": "eslint --max-warnings=0 --ext=js src",
    "lint-fix": "eslint --max-warnings=0 --fix --ext=js src",
    "prettier": "prettier --check ./src",
    "prettier-fix": "prettier --write ./src",
    "prettier-lint-fix": "npm run prettier-fix && npm run lint-fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/desholmes/p5js-circles"
  },
  "author": "Des Holmes https://github.com/desholmes",
  "license": "MIT",
  "dependencies": {
    "p5": "1.5.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.0",
    "dat.gui": "0.7.9",
    "eslint": "8.28.0",
    "eslint-config-p5js": "1.1.0",
    "eslint-config-prettier": "^8.5.0",
    "parcel": "2.8.0",
    "prettier": "2.7.1"
  }
}
